
    <h1>Article List</h1>
    <h1><a href="/dashboard">dashboard</a></h1>
    <ul>
        <% articles.forEach((article) => { %>
            <li>
                <img src="../uploads/<%= article.photo %>" width="100px" height="100px" alt="">
                <h2><%= article.title %></h2>
                <p><%= article.content %></p>
                <p><%= article.publishDate %></p>
                <p><%= article.updatedAt  %></p>
                <p><%= article.author.fullName %></p>

                    <a href="/articles/<%= article.id %>">show</a>

            </li>
            <hr />
        <% }) %>
    </ul>
